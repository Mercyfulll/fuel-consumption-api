<div class="container">

    <h3>Vehicles Available<h3>
    <table>
        <tr>
            <th>Description</th>
            <th>Total distance travelled</th>
            <th>Total fuel spent</th>
            <th>Fuel compustion</th>
        </tr>
        {{#each cars}}
        <tr>
            <td>{{this.description}}</td>
            <td>{{this.total_distance}}</td>
            <td>{{this.total_amount}}</td>
            <td>{{this.fuel_consumption}}</td>
        </tr>
        {{/each}}
    </table>


</div>